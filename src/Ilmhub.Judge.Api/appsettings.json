{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace"
    }
  },
  "RateLimiting": {
    "Enabled": true,
    "Permit": 1,
    "Window": 1,
    "QueueLimit": 1
  },
  "OpenTelemetry": {
    "ServiceName": "Ilmhub.Judge.Api",
    "Driver": "Jaeger"
  },
  "Judger": {
    "RootFolder": "/judger",
    "SystemUsers": {
      "Compiler": {
        "Username": "compiler",
        "UserId": 2000,
        "GroupId": 2000
      },
      "Runner": {
        "Username": "runner",
        "UserId": 2001,
        "GroupId": 2001
      }
    },
    "LanguageConfigurations": [
      {
        "Id": 1,
        "Name": "C",
        "Compile": {
          "SourceName": "main.c",
          "ExecutableName": "main",
          "MaxCpuTime": 3000,
          "MaxRealTime": 5000,
          "MaxMemory": 134217728,
          "Command": "/usr/bin/gcc",
          "Arguments": [
            "{src_path}",
            "-DONLINE_JUDGE",
            "-O2",
            "-fmax-errors=3",
            "-std=c99",
            "-lm",
            "-o",
            "{exe_path}"
          ]
        },
        "Run": {
          "Command": "{exe_path}",
          "SeccompRule": "c_cpp"
        }
      },
      {
        "Id": 2,
        "Name": "C++",
        "Compile": {
          "SourceName": "main.cpp",
          "ExecutableName": "main",
          "MaxCpuTime": 3000,
          "MaxRealTime": 5000,
          "MaxMemory": 134217728,
          "Command": "/usr/bin/g++",
          "Arguments": [
            "{src_path}",
            "-DONLINE_JUDGE",
            "-O2",
            "-w",
            "-fmax-errors=3",
            "-std=c++11",
            "-lm",
            "-o",
            "{exe_path}"
          ]
        },
        "Run": {
          "Command": "{exe_path}",
          "SeccompRule": "c_cpp",
          "EnvironmentVariables": [
            "LANG=en_US.UTF-8",
            "LANGUAGE=en_US:en",
            "LC_ALL=en_US.UTF-8"
          ]
        }
      },
      {
        "Id": 3,
        "Name": "C# (mono)",
        "Compile": {
          "SourceName": "Main.cs",
          "ExecutableName": "Main",
          "MaxCpuTime": 3000,
          "MaxRealTime": 5000,
          "MaxMemory": -1,
          "Command": "/usr/bin/mcs",
          "Arguments": [
            "-out:{exe_path}",
            "{src_path}"
          ]
        },
        "Run": {
          "Command": "/usr/bin/mono",
          "SeccompRule": "",
          "Arguments": [
            "{exe_path}"
          ],
          "EnvironmentVariables": [
            "LANG=en_US.UTF-8",
            "LANGUAGE=en_US:en",
            "LC_ALL=en_US.UTF-8"
          ],
          "MemoryLimitCheckOnly": true
        }
      },
      {
        "Id": 4,
        "Name": "Python (3)",
        "Compile": {
          "SourceName": "solution.py",
          "ExecutableName": "__pycache__/solution.cpython-36.pyc",
          "MaxCpuTime": 3000,
          "MaxRealTime": 5000,
          "MaxMemory": 134217728,
          "Command": "/usr/bin/python3",
          "Arguments": [
            "-m",
            "py_compile",
            "{src_path}"
          ]
        },
        "Run": {
          "Command": "/usr/bin/python3",
          "Arguments": [
            "{exe_path}"
          ],
          "SeccompRule": "general",
          "EnvironmentVariables": [
            "PYTHONIOENCODING=UTF-8",
            "LANG=en_US.UTF-8",
            "LANGUAGE=en_US:en",
            "LC_ALL=en_US.UTF-8"
          ]
        }
      },
      {
        "Id": 6,
        "Name": ".NET 6",
        "Compile": {
          "SourceName": "Program.cs",
          "DotnetProjectPath": "/judger/dotnet/net6/net6.csproj",
          "Command": "/usr/bin/dotnet",
          "Arguments": [
            "publish",
            "{src_path}",
            "-c",
            "Release",
            "-o",
            "{exe_path}",
            "--no-self-contained",
            "-r",
            "linux-x64",
            "-p:PublishSingleFile=true"
          ]
        },
        "Run": {
          "Command": "{exe_path}",
          "SeccompRule": "general"
        }
      },
      {
        "Id": 7,
        "Name": ".NET 7",
        "Compile": {
          "SourceName": "Program.cs",
          "DotnetProjectPath": "/judger/dotnet/net7/net7.csproj",
          "Command": "/usr/bin/dotnet",
          "Arguments": [
            "publish",
            "{src_path}",
            "-c",
            "Release",
            "-o",
            "{exe_path}",
            "--no-self-contained",
            "-r",
            "linux-x64",
            "-p:PublishSingleFile=true"
          ]
        },
        "Run": {
          "Command": "{exe_path}",
          "SeccompRule": "general"
        }
      },
      {
        "Id": 100,
        "Name": "Go",
        "Compile": {
          "SourceName": "main.go",
          "ExecutableName": "main",
          "Command": "/usr/bin/go",
          "Arguments": [
            "build",
            "-o",
            "{exe_path}",
            "{src_path}"
          ],
          "EnvironmentVariables": [
            "GOCACHE=/tmp",
            "PATH='$PATH'"
          ]
        },
        "Run": {
          "Command": "{exe_path}",
          "SeccompRule": "",
          "EnvironmentVariables": [
            "GODEBUG=madvdontneed=1",
            "GOCACHE=off",
            "LANG=en_US.UTF-8",
            "LANGUAGE=en_US:en",
            "LC_ALL=en_US.UTF-8"
          ],
          "MemoryLimitCheckOnly": true
        }
      }
    ]
  }
}